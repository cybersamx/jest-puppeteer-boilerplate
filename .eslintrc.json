{
  "extends": ["airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "es6": true,
    "browser": false,
    "jasmine": true,
    "node": true,
    "commonjs": false,
    "jest": true
  },
  "plugins": ["prettier"]
}